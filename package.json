{
  "name": "monotracker",
  "version": "1.0.4",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --print-label --parallel dev:*",
    "dev:start": "webpack-dev-server",
    "dev:electron": "export MODE=develop; electron --trace-warnings  .",
    "build": "webpack",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "dexatime.test.1",
    "productName": "MonoTracker",
    "mac": {
      "icon": "./build/icon.png"
    },
    "linux": {
      "category": "Development",
      "description": "MonoTracker",
      "icon": "./build/icon.png",
      "target": [
        "AppImage"
      ]
    }
  },
  "postinstall": "electron-builder install-app-deps",
  "author": "Artem Klymov <art.klimoff@gmail.com>",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.19.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "css-loader": "^3.5.3",
    "electron": "^21.3.3",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9",
    "extract-loader": "2.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "sass-loader": "^13.2.0",
    "style-loader": "0.23.0"
  },
  "dependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "babel-loader": "^9.1.0",
    "better-sqlite3": "^8.0.1",
    "classnames": "^2.2.6",
    "config": "^3.3.1",
    "electron-localshortcut": "^1.1.1",
    "fs": "^0.0.1-security",
    "menubar": "^9.2.3",
    "moment": "^2.27.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-moment": "^1.1.2",
    "react-redux": "^8.0.5",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "sass": "^1.57.1",
    "uuid": "^8.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
